{
  "name": "server",
  "version": "1.0.0",
  "author": "",
  "license": "Apache-2.0",
  "scripts": {
    "inspect": "npm run pre-run && node inspect ./bin/www",
    "lint": "eslint .",
    "lint-fix": "npm run lint -- --fix",
    "load-db": "node ./utils/load-mongodb",
    "pre-start": "source ./env/env.sh",
    "start": "pm2 start ./bin/www",
    "start-dev": "nodemon --verbose --ignore 'public/*' -e js,pug,css,json ./bin/www",
    "start-dev-inspect": "nodemon --inspect --verbose -e js,pug,css,json ./bin/www",
    "test": "mocha ./test/test-rest-services"
  },
  "bugs": {
    "url": ""
  },
  "homepage": "",
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "node",
    "npm"
  ],
  "dependencies": {
    "ajv": "^6.10.2",
    "async": "^2.6.1",
    "compression": "^1.7.4",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.5",
    "dateformat": "^3.0.3",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.0",
    "express-validator": "^5.3.1",
    "geojson": "^0.5.0",
    "http-errors": "~1.6.2",
    "log4js": "^3.0.2",
    "mongodb": "^3.3.4",
    "mongoose": "^5.6.7",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "mysql": "^2.17.1",
    "pug": "^2.0.0",
    "sharp": "^0.23.3",
    "sqlite3": "^4.0.2",
    "uuid": "^3.3.3",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "chai": "^4.1.2",
    "eslint": "^5.2.0",
    "eslint-config-google": "^0.9.1",
    "mocha": "^5.2.0",
    "prettier": "2.2.1"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "google"
    ],
    "env": {
      "node": true,
      "es6": true
    },
    "parserOptions": {
      "ecmaVersion": 2017
    },
    "rules": {
      "max-len": [
        2,
        120,
        4,
        {
          "ignoreUrls": true
        }
      ],
      "spaced-comment": 0,
      "no-trailing-spaces": 0,
      "comma-dangle": 0,
      "object-curly-spacing": 0,
      "array-bracket-spacing": 0,
      "no-console": 0
    }
  },
  "eslintIgnore": [
    "node_modules"
  ]
}
